{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/po/:city",
        "dest": "/api/po/:city"
      },
      {
        "src": "/op/:city",
        "dest": "/api/op/:city"
      }
    ]
  }