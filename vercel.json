{
  "version": 2,
  "builds": [
    { "src": "/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/po", "dest": "/index.js" },
    { "src": "/po/(.*)", "dest": "/index.js" },
    { "src": "/op", "dest": "/index.js" },
    { "src": "/op/(.*)", "dest": "/index.js" },
    { "src": "/total-prices", "dest": "/index.js" },
    { "src": "/exchange-rates", "dest": "/index.js" }
  ]
}
